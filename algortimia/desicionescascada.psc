Proceso desicionescascada
	//desiciones en cascada
	Si 3>4 Entonces
		escribir "verdadero";
	Sino
		Si 3>5 Entonces
			escribir "verdadero";
		Sino
			Si 3>6 Entonces
				escribir "verdadero";
			Sino
				Si 3>7 Entonces
					escribir "verdadero";
				Sino
					Si 3>8 Entonces
						escribir "verdadero";
					Sino
						Si 3>1 Entonces
							escribir "verdadero";
						Sino
							escribir "falso";
						FinSi
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi
FinProceso
